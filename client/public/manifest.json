{
  "manifest_version": 3,
  "name": "Send Email",
  "description": "Sends Email",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_title": "Open"
  },
  "permissions": [
    "scripting"
  ],
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "https://*/*"
      ]
    }
  ]
}